        "custom_liquid": "<!DOCTYPE html>\n<html lang=\"pt-BR\">\n  <head>\n    <meta charset=\"UTF-8\" />\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n    <title>Carrossel de Ingredientes Herbais</title>\n    <style>\n      * {\n        margin: 0;\n        padding: 0;\n        box-sizing: border-box;\n      }\n\n      body {\n        font-family: \"Segoe UI\", Tahoma, Geneva, Verdana, sans-serif;\n      }\n\n      .carousel-container {\n        width: 100%;\n        background: #f4e4d1;\n\n        padding: 40px 20px;\n        box-shadow: 0 10px 40px rgba(0, 0, 0, 0.1);\n      }\n\n      h1 {\n        text-align: center;\n        color: #2c1810;\n        font-size: clamp(1.5rem, 4vw, 2.5rem);\n        margin-bottom: 40px;\n        text-transform: uppercase;\n        font-weight: 900;\n        letter-spacing: -1px;\n        line-height: 1.2;\n      }\n\n      .carousel-wrapper {\n        /* overflow: hidden; */\n        border-radius: 15px;\n        position: relative;\n      }\n\n      .carousel-track {\n        display: flex;\n        transition: none;\n      }\n\n      .herb-card {\n        background: white;\n        border-radius: 20px;\n        padding: 30px;\n        max-width: 350px;\n        margin: 0 15px;\n        box-shadow: 0 5px 20px rgba(0, 0, 0, 0.1);\n        text-align: center;\n        transition: transform 0.3s ease;\n        flex-shrink: 0;\n      }\n\n      .herb-card:hover {\n        transform: translateY(-5px) scale(1.02);\n        box-shadow: 0 8px 30px rgba(0, 0, 0, 0.15);\n      }\n\n      .herb-image {\n        width: 120px;\n        height: 120px;\n        margin: 0 auto 20px;\n        background: #e8f5e9;\n        border-radius: 50%;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        font-size: 60px;\n      }\n\n      .herb-name {\n        font-size: 1.8rem;\n        color: #2c1810;\n        margin-bottom: 15px;\n        font-weight: 700;\n      }\n\n      .herb-description {\n        font-size: 1rem;\n        color: #5a4a42;\n        line-height: 1.6;\n      }\n\n      /* Efeito de gradiente nas bordas */\n      .carousel-wrapper::before,\n      .carousel-wrapper::after {\n        content: \"\";\n        position: absolute;\n        top: 0;\n        width: 100px;\n        height: 100%;\n        z-index: 2;\n        pointer-events: none;\n      }\n\n      .carousel-wrapper::before {\n        left: 0;\n        /* background: linear-gradient(to right, #f4e4d1, transparent); */\n      }\n\n      .carousel-wrapper::after {\n        right: 0;\n        /* background: linear-gradient(to left, #f4e4d1, transparent); */\n      }\n\n      /* Layout em grade para telas maiores que 650px */\n      @media (min-width: 651px) {\n        .carousel-wrapper {\n          overflow: visible !important;\n        }\n\n        .carousel-wrapper::before,\n        .carousel-wrapper::after {\n          display: none;\n        }\n\n        .carousel-track {\n          flex-wrap: wrap;\n          justify-content: center;\n          gap: 20px;\n          transform: none !important;\n        }\n\n        .carousel-track.static-layout .herb-card {\n          margin: 10px;\n          flex-shrink: 1;\n          flex-basis: calc(33.333% - 40px);\n          min-width: 280px;\n        }\n      }\n\n      /* Responsivo */\n      @media (max-width: 768px) {\n        .herb-card {\n          /* min-width: 280px; */\n          padding: 20px;\n        }\n\n        .herb-image {\n          width: 100px;\n          height: 100px;\n          font-size: 50px;\n        }\n\n        .herb-name {\n          font-size: 1.5rem;\n        }\n\n        .herb-description {\n          font-size: 0.9rem;\n        }\n      }\n\n      @media (max-width: 480px) {\n        .herb-card {\n          /* min-width: 250px; */\n        }\n\n        h1 {\n          font-size: 1.3rem;\n          margin-bottom: 30px;\n        }\n      }\n    </style>\n  </head>\n  <body>\n    <div class=\"carousel-container\">\n      <h1>100% Wild-Crafted, Naturopathic-Approved Herbal Ingredients</h1>\n\n      <div class=\"carousel-wrapper\">\n        <div class=\"carousel-track\" id=\"carouselTrack\">\n          <!-- Cards ser√£o inseridos via JavaScript -->\n        </div>\n      </div>\n    </div>\n\n    <script>\n      const herbs = [\n        {\n          emoji: \"üåø\",\n          name: \"Sarsaparilla Root\",\n          description:\n            \"Treats joint pain, heals skin conditions such as psoriasis and eczema, and purifies blood.\",\n        },\n        {\n          emoji: \"üå∏\",\n          name: \"Burdock Root\",\n          description:\n            \"Improves skin conditions and eliminates toxins in the body.\",\n        },\n        {\n          emoji: \"üçá\",\n          name: \"Elder Berry\",\n          description:\n            \"Prevents constipation, treats colds, infections, and fever, and boosts immunity.\",\n        },\n        {\n          emoji: \"üå±\",\n          name: \"Dandelion Root\",\n          description:\n            \"Supports liver health, aids digestion, and provides powerful antioxidants for overall wellness.\",\n        },\n        {\n          emoji: \"üçÉ\",\n          name: \"Nettle Leaf\",\n          description:\n            \"Rich in vitamins and minerals, reduces inflammation, and supports kidney function.\",\n        },\n      ]\n\n      const track = document.getElementById(\"carouselTrack\")\n      let position = 0\n      let isPaused = false\n      const speed = 0.8 // pixels por frame\n      let animationId = null\n\n      // Criar cards\n      function createCard(herb) {\n        return `\n                <div class=\"herb-card\">\n                    <div class=\"herb-image\">${herb.emoji}</div>\n                    <h2 class=\"herb-name\">${herb.name}</h2>\n                    <p class=\"herb-description\">${herb.description}</p>\n                </div>\n            `\n      }\n\n      // Verificar se deve usar carrossel ou layout est√°tico\n      function shouldUseCarousel() {\n        return window.innerWidth <= 650\n      }\n\n      // Inicializar carrossel\n      function initCarousel() {\n        if (shouldUseCarousel()) {\n          // Modo carrossel para telas pequenas\n          track.classList.remove(\"static-layout\")\n\n          // Criar cards suficientes para preencher a tela e criar loop\n          let htmlContent = \"\"\n          const numCopies = Math.ceil(window.innerWidth / 200) + 5 // Garantir cards suficientes\n\n          for (let i = 0; i < numCopies; i++) {\n            herbs.forEach((herb) => {\n              htmlContent += createCard(herb)\n            })\n          }\n\n          track.innerHTML = htmlContent\n\n          // Reiniciar anima√ß√£o se n√£o estiver rodando\n          if (!animationId) {\n            animate()\n          }\n        } else {\n          // Modo est√°tico para telas grandes\n          track.classList.add(\"static-layout\")\n\n          // Cancelar anima√ß√£o se estiver rodando\n          if (animationId) {\n            cancelAnimationFrame(animationId)\n            animationId = null\n          }\n\n          // Criar apenas uma c√≥pia de cada card\n          let htmlContent = \"\"\n          herbs.forEach((herb) => {\n            htmlContent += createCard(herb)\n          })\n\n          track.innerHTML = htmlContent\n          track.style.transform = \"none\"\n        }\n      }\n\n      // Animar carrossel\n      function animate() {\n        if (shouldUseCarousel()) {\n          if (!isPaused) {\n            position -= speed\n            track.style.transform = `translateX(${position}px)`\n\n            // Calcular quando resetar\n            const cardWidth = 365 // largura do card + margem\n            const resetPoint = -(herbs.length * cardWidth)\n\n            if (position <= resetPoint) {\n              position = 0\n              track.style.transition = \"none\"\n              track.style.transform = `translateX(${position}px)`\n            }\n          }\n\n          animationId = requestAnimationFrame(animate)\n        }\n      }\n\n      // Pausar ao passar o mouse (apenas em modo carrossel)\n      track.addEventListener(\"mouseenter\", () => {\n        if (shouldUseCarousel()) {\n          isPaused = true\n        }\n      })\n\n      track.addEventListener(\"mouseleave\", () => {\n        if (shouldUseCarousel()) {\n          isPaused = false\n        }\n      })\n\n      // Touch para mobile\n      let touchStartX = 0\n      let touchCurrentX = 0\n      let isDragging = false\n\n      track.addEventListener(\"touchstart\", (e) => {\n        if (shouldUseCarousel()) {\n          touchStartX = e.touches[0].clientX\n          isDragging = true\n          isPaused = true\n        }\n      })\n\n      track.addEventListener(\"touchmove\", (e) => {\n        if (!isDragging || !shouldUseCarousel()) return\n        touchCurrentX = e.touches[0].clientX\n        const diff = touchCurrentX - touchStartX\n        track.style.transform = `translateX(${position + diff}px)`\n      })\n\n      track.addEventListener(\"touchend\", () => {\n        if (shouldUseCarousel()) {\n          isDragging = false\n          isPaused = false\n          position += touchCurrentX - touchStartX\n        }\n      })\n\n      // Ajustar velocidade baseado na prefer√™ncia do usu√°rio\n      const prefersReducedMotion = window.matchMedia(\n        \"(prefers-reduced-motion: reduce)\"\n      )\n      if (prefersReducedMotion.matches) {\n        isPaused = true\n      }\n\n      // Inicializar\n      initCarousel()\n\n      // Reinicializar ao redimensionar\n      window.addEventListener(\"resize\", () => {\n        position = 0\n        initCarousel()\n      })\n    </script>\n  </body>\n</html>",
